        <![endif]-->
        title Angular Pure Datepicker
    body
        #content_view.container
            #demo_page(ng-controller="DemoPageCtrl")
                section
                    hgroup
                        h1 Common case
                    .case_description
                        ul
                            li Valid ngModel
                    form#demo_simple_form(name="demo_simple_form")
                        .form-group
                            label.control-label(for="apd_1") Date
                            pure-datepicker#apd_1(ng-model="resultModel")
                    section.well
                        div
                            span Day:
                            | &nbsp;
                            span(ng-bind="resultModel.day")
                        div
                            span Month:
                            | &nbsp;
                            span(ng-bind="resultModel.month")
                        div
                            span Year:
